	<queryString>
		<![CDATA[SELECT
	c.nome nome_cliente,
	v.id,
	v.identificador,
	v.valor_total,
	v.desconto,
	v.cancelada,
	sessao.identificador
FROM venda v 
	join cliente c ON 
	 v.fk_cliente = c.id 
	INNER JOIN sessao ON 
	 v.fk_sessao = sessao.id]]>
	</queryString>
